  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node -r ./newrelic.js dist/src/main",
    "lint": "biome check --diagnostic-level=error",
    "test": "npx vitest run",
    "test:watch": "npx vitest",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:reset": "npx prisma migrate reset --force",
    "seed": "npx prisma db seed",
    "seed:all": "npx prisma db seed -- --type all",
    "seed:csv": "npx prisma db seed -- --type csv",
    "generate": "npx prisma generate",
    "prisma-format": "for file in prisma/schema/*.prisma; do npx prisma-case-format -c prisma/.prisma-case-format.yaml -f \"$file\"; done",
    "codegen": "graphql-codegen",
    "test:e2e": "playwright test --config=e2e-tests/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=e2e-tests/playwright.config.ts --ui",
    "test:e2e:debug": "playwright test --config=e2e-tests/playwright.config.ts --debug",
    "test:e2e:report": "playwright show-report e2e-tests/playwright-report",
    "install:clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "prisma": {
    "seed": "tsx ./seed/seed.ts",
    "schema": "prisma/schema"
  },
