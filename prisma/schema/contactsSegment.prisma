model ContactsSegment {
  id                         Int      @id @default(autoincrement())
  createdAt                  DateTime @default(now()) @map("created_at")
  updatedAt                  DateTime @updatedAt @map("updated_at")
  name                       String
  genderMale                 Boolean  @default(false) @map("gender_male")
  genderFemale               Boolean  @default(false) @map("gender_female")
  genderUnknown              Boolean  @default(false) @map("gender_unknown")
  age18_25                   Boolean  @default(false) @map("age_18_25")
  age25_35                   Boolean  @default(false) @map("age_25_35")
  age35_50                   Boolean  @default(false) @map("age_35_50")
  age50Plus                  Boolean  @default(false) @map("age_50_plus")
  politicalPartyDemocrat     Boolean  @default(false) @map("political_party_democrat")
  politicalPartyNonPartisan  Boolean  @default(false) @map("political_party_non_partisan")
  politicalPartyRepublican   Boolean  @default(false) @map("political_party_republican")
  address                    Boolean  @default(false) @map("address")
  cellPhone                  Boolean  @default(false) @map("cell_phone")
  landline                   Boolean  @default(false) @map("landline")
  email                      Boolean  @default(false) @map("email")
  emailNotListed             Boolean  @default(false) @map("email_not_listed")
  hasAddress                 Boolean  @default(false) @map("has_address")
  addressNotListed           Boolean  @default(false) @map("address_not_listed")
  hasCellPhone               Boolean  @default(false) @map("has_cell_phone")
  cellPhoneNotListed         Boolean  @default(false) @map("cell_phone_not_listed")
  hasLandline                Boolean  @default(false) @map("has_landline")
  landlineNotListed          Boolean  @default(false) @map("landline_not_listed")
  hasEmail                   Boolean  @default(false) @map("has_email")
  registeredVoter            Boolean  @default(false) @map("registered_voter")
  registeredVoterYes         Boolean  @default(false) @map("registered_voter_yes")
  registeredVoterNo          Boolean  @default(false) @map("registered_voter_no")
  activeVoterYes             Boolean  @default(false) @map("active_voter_yes")
  activeVoterNo              Boolean  @default(false) @map("active_voter_no")
  voterLikelyFirstTime       Boolean  @default(false) @map("voter_likely_first_time")
  voterLikelyLikely          Boolean  @default(false) @map("voter_likely_likely")
  voterLikelySuper           Boolean  @default(false) @map("voter_likely_super")
  voterLikelyUnknown         Boolean  @default(false) @map("voter_likely_unknown")
  maritalStatusMarried       Boolean  @default(false) @map("marital_status_married")
  maritalStatusLikelyMarried Boolean  @default(false) @map("marital_status_likely_married")
  maritalStatusSingle        Boolean  @default(false) @map("marital_status_single")
  maritalStatusLikelySingle  Boolean  @default(false) @map("marital_status_likely_single")
  maritalStatusUnknown       Boolean  @default(false) @map("marital_status_unknown")
  hasChildrenNo              Boolean  @default(false) @map("has_children_no")
  hasChildrenYes             Boolean  @default(false) @map("has_children_yes")
  hasChildrenUnknown         Boolean  @default(false) @map("has_children_unknown")
  veteranStatusYes           Boolean  @default(false) @map("veteran_status_yes")
  veteranStatusNo            Boolean  @default(false) @map("veteran_status_no")
  veteranStatusUnknown       Boolean  @default(false) @map("veteran_status_unknown")
  businessOwnerYes           Boolean  @default(false) @map("business_owner_yes")
  businessOwnerLikely        Boolean  @default(false) @map("business_owner_likely")
  businessOwnerNo            Boolean  @default(false) @map("business_owner_no")
  businessOwnerUnknown       Boolean  @default(false) @map("business_owner_unknown")
  educationHighSchool        Boolean  @default(false) @map("education_high_school")
  educationSomeCollege       Boolean  @default(false) @map("education_some_college")
  educationTechnicalSchool   Boolean  @default(false) @map("education_technical_school")
  educationSomeCollegeDegree Boolean  @default(false) @map("education_some_college_degree")
  educationCollegeDegree     Boolean  @default(false) @map("education_college_degree")
  educationGraduateDegree    Boolean  @default(false) @map("education_graduate_degree")
  educationUnknown           Boolean  @default(false) @map("education_unknown")
  householdIncome15_25k      Boolean  @default(false) @map("household_income_15_25k")
  householdIncome25_35k      Boolean  @default(false) @map("household_income_25_35k")
  householdIncome35_50k      Boolean  @default(false) @map("household_income_35_50k")
  householdIncome50_75k      Boolean  @default(false) @map("household_income_50_75k")
  householdIncome75_100k     Boolean  @default(false) @map("household_income_75_100k")
  householdIncome100_125k    Boolean  @default(false) @map("household_income_100_125k")
  householdIncome125_150k    Boolean  @default(false) @map("household_income_125_150k")
  householdIncome150_175k    Boolean  @default(false) @map("household_income_150_175k")
  householdIncome175_200k    Boolean  @default(false) @map("household_income_175_200k")
  householdIncome200_250k    Boolean  @default(false) @map("household_income_200_250k")
  householdIncome250kPlus    Boolean  @default(false) @map("household_income_250k_plus")
  householdIncomeUnknown     Boolean  @default(false) @map("household_income_unknown")
  languageEnglish            Boolean  @default(false) @map("language_english")
  languageSpanish            Boolean  @default(false) @map("language_spanish")
  languageOther              Boolean  @default(false) @map("language_other")
  ethnicityCaucasian         Boolean  @default(false) @map("ethnicity_caucasian")
  ethnicityAfricanAmerican   Boolean  @default(false) @map("ethnicity_african_american")
  ethnicityAsian             Boolean  @default(false) @map("ethnicity_asian")
  ethnicityEuropean          Boolean  @default(false) @map("ethnicity_european")
  ethnicityHispanic          Boolean  @default(false) @map("ethnicity_hispanic")
  ethnicityUnknown           Boolean  @default(false) @map("ethnicity_unknown")

  campaignId Int      @map("campaign_id")
  campaign   Campaign @relation(fields: [campaignId], references: [id], onDelete: Cascade)

  @@unique([name, campaignId])
  @@map("contacts_segment")
}
