model VoterFileFilter {
  id                         Int        @id @default(autoincrement())
  createdAt                  DateTime   @default(now()) @map("created_at")
  updatedAt                  DateTime   @updatedAt @map("updated_at")
  name                       String?
  audienceSuperVoters        Boolean?   @default(false) @map("audience_super_voters")
  audienceLikelyVoters       Boolean?   @default(false) @map("audience_likely_voters")
  audienceUnreliableVoters   Boolean?   @default(false) @map("audience_unreliable_voters")
  audienceUnlikelyVoters     Boolean?   @default(false) @map("audience_unlikely_voters")
  audienceFirstTimeVoters    Boolean?   @default(false) @map("audience_first_time_voters")
  partyIndependent           Boolean?   @default(false) @map("party_independent")
  partyDemocrat              Boolean?   @default(false) @map("party_democrat")
  partyRepublican            Boolean?   @default(false) @map("party_republican")
  age18_25                   Boolean?   @default(false) @map("age_18_25")
  age25_35                   Boolean?   @default(false) @map("age_25_35")
  age35_50                   Boolean?   @default(false) @map("age_35_50")
  age50Plus                  Boolean?   @default(false) @map("age_50_plus")
  genderMale                 Boolean?   @default(false) @map("gender_male")
  genderFemale               Boolean?   @default(false) @map("gender_female")
  genderUnknown              Boolean?   @default(false) @map("gender_unknown")
  hasCellPhone               Boolean?   @default(false) @map("has_cell_phone")
  hasLandline                Boolean?   @default(false) @map("has_landline")
  languageCodes              String[]   @default([]) @map("language_codes")
  voterStatus                String[]   @default([]) @map("voter_status")
  likelyMarried              Boolean?   @default(false) @map("likely_married")
  likelySingle               Boolean?   @default(false) @map("likely_single")
  married                    Boolean?   @default(false) @map("married")
  single                     Boolean?   @default(false) @map("single")
  maritalUnknown             Boolean?   @default(false) @map("marital_unknown")
  veteranYes                 Boolean?   @default(false) @map("veteran_yes")
  veteranUnknown             Boolean?   @default(false) @map("veteran_unknown")
  educationNone              Boolean?   @default(false) @map("education_none")
  educationHighSchoolDiploma Boolean?   @default(false) @map("education_high_school_diploma")
  educationTechnicalSchool   Boolean?   @default(false) @map("education_technical_school")
  educationSomeCollege       Boolean?   @default(false) @map("education_some_college")
  educationCollegeDegree     Boolean?   @default(false) @map("education_college_degree")
  educationGraduateDegree    Boolean?   @default(false) @map("education_graduate_degree")
  educationUnknown           Boolean?   @default(false) @map("education_unknown")
  incomeRanges               String[]   @default([]) @map("income_ranges")
  ethnicityAsian             Boolean?   @default(false) @map("ethnicity_asian")
  ethnicityEuropean          Boolean?   @default(false) @map("ethnicity_european")
  ethnicityHispanic          Boolean?   @default(false) @map("ethnicity_hispanic")
  ethnicityAfricanAmerican   Boolean?   @default(false) @map("ethnicity_african_american")
  ethnicityOther             Boolean?   @default(false) @map("ethnicity_other")
  ethnicityUnknown           Boolean?   @default(false) @map("ethnicity_unknown")
  businessOwnerYes           Boolean?   @default(false) @map("business_owner_yes")
  businessOwnerUnknown       Boolean?   @default(false) @map("business_owner_unknown")
  registeredVoterTrue        Boolean?   @default(false) @map("registered_voter_true")
  registeredVoterFalse       Boolean?   @default(false) @map("registered_voter_false")
  hasChildrenYes             Boolean?   @default(false) @map("has_children_yes")
  hasChildrenNo              Boolean?   @default(false) @map("has_children_no")
  hasChildrenUnknown         Boolean?   @default(false) @map("has_children_unknown")
  homeownerYes               Boolean?   @default(false) @map("homeowner_yes")
  homeownerLikely            Boolean?   @default(false) @map("homeowner_likely")
  homeownerNo                Boolean?   @default(false) @map("homeowner_no")
  homeownerUnknown           Boolean?   @default(false) @map("homeowner_unknown")
  voterCount                 Int?       @map("voter_count")
  campaign                   Campaign   @relation(fields: [campaignId], references: [id], onDelete: Cascade)
  campaignId                 Int        @map("campaign_id")
  outreaches                 Outreach[]

  @@index([campaignId])
  @@index([id, campaignId])
  @@map("voter_file_filter")
}
