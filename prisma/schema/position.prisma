model Position {
  id                Int                @id @default(autoincrement())
  name              String             @unique
  topIssue          TopIssue?          @relation(fields: [topIssueId], references: [id], onDelete: Cascade)
  topIssueId        Int?               @map("top_issue_id")
  campaignPositions CampaignPosition[]

  @@map("position")
}
